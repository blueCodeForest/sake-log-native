{
  "expo": {
    "name": "酒レコ",
    "slug": "sake-rec",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sake-rec",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shun-kimura.sake-rec",
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shun_kimura.sake_rec"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "personal-4ai",
          "project": "sake-rec",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "personal-4ai",
            "project": "sake-rec"
          }
        }
      ]
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a4f88ea9-96f0-4843-9395-0da3c0bb414a"
      }
    },
    "owner": "shuuuun",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a4f88ea9-96f0-4843-9395-0da3c0bb414a"
    }
  }
}
